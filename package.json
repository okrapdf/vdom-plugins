{
  "name": "@okrapdf/vdom-plugins",
  "version": "0.1.0",
  "description": "VDOM Plugin System - Chrome Extension-like plugins for document analysis",
  "private": true,
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./runtime": {
      "types": "./src/runtime.ts",
      "import": "./src/runtime.ts"
    },
    "./plugins/orphan-detector": {
      "types": "./src/plugins/orphan-detector/index.ts",
      "import": "./src/plugins/orphan-detector/index.ts"
    },
    "./plugins/qwen-markdown": {
      "types": "./src/plugins/qwen-markdown/index.ts",
      "import": "./src/plugins/qwen-markdown/index.ts"
    }
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "keywords": [
    "vdom",
    "plugin",
    "document",
    "ocr",
    "pdf"
  ],
  "peerDependencies": {
    "react": ">=18.0.0"
  }
}
